{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "export NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service build",
        "outputDirectory": "dist",
        "nodeVersion": "18"
      }
    }
  ],
  "env": {
    "NODE_OPTIONS": "--openssl-legacy-provider"
  }
} 